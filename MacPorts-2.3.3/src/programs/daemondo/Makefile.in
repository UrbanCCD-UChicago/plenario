# $Id: Makefile.in 117619 2014-03-05 20:53:43Z cal@macports.org $

srcdir     = @srcdir@
VPATH      = @srcdir@

include ../../../Mk/macports.autoconf.mk

datarootdir=	@datarootdir@
mandir=		@mandir@
mandircat8=	${mandir}/cat8

CFLAGS_PEDANTIC	=
CPPFLAGS	+= -I../..
SRCS		= main.c
OBJS		= main.o
LIBS		= -framework CoreFoundation \
			  -framework SystemConfiguration \
			  -framework IOKit
PROGRAM		= build/daemondo

all: daemondo

daemondo: mkdirs ${PROGRAM}

${PROGRAM}: ${OBJS}
	${CC} ${CFLAGS} -o ${PROGRAM} ${LIBS} ${OBJS}

mkdirs:
	mkdir -p build

clean:
	rm -Rf build ${PROGRAM} ${OBJS}

distclean: clean
	rm -f Makefile

install: daemondo
	${INSTALL} -o ${DSTUSR} -g ${DSTGRP} -m 555 ${PROGRAM} ${DESTDIR}${bindir}

test:
